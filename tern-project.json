{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "chai",
    "ecmascript",
    "jquery",
    "underscore"
  ],
  "loadEagerly": [
    "node_modules/react/**/*.js"
  ],
  "dontLoad": [
    "node_modules/**"
  ],
  "plugins": {
    "modules": {},
    "requirejs": {},
    "es_modules": {},
    "node": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }  
  }
}
